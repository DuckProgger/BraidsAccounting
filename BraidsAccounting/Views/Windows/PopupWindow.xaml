<Window x:Class="BraidsAccounting.Views.Windows.PopupWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BraidsAccounting.Views.Windows"
        xmlns:mod="clr-namespace:BraidsAccounting.Modules"  
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="PopupWindow" 
        SizeToContent="WidthAndHeight"
        WindowStartupLocation="CenterScreen"
        Width="100" Height="100">
    <Grid>
        <ContentControl prism:RegionManager.RegionName="{x:Static mod:RegionNames.Popup}" />
    </Grid>
</Window>
