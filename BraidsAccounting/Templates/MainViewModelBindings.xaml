<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:BraidsAccounting.ViewModels"
                    xmlns:view="clr-namespace:BraidsAccounting.Views">


    <DataTemplate DataType="{x:Type vm:StoreViewModel}">
        <view:StoreView DataContext="{Binding}"/>
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ServiceViewModel}">
        <view:ServiceView DataContext="{Binding}"/>
    </DataTemplate>

</ResourceDictionary>